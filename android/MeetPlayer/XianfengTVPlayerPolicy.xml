<?xml version="1.0" encoding="utf-8"?>
<Policy>
	<DeviceDesc>XianfengTV</DeviceDesc>
	<DeviceId>100456</DeviceId>
	<Protocol>http</Protocol>
	<Picture>
		<Ext>bmp,gif,jpeg,jpg,png,webp,mpo,jps,pns</Ext>
	</Picture>
	<Music>
		<Ext>flac,mp3,mp2,ape</Ext>
	</Music>
	<Video>
		<Ext>mp4,3gp,mov</Ext>
		<Demuxer></Demuxer>
		<VideoCodec>hevc,h264,h263,mpeg4,vc1,vp8,mjpeg,wmv3</VideoCodec>
		<AudioCodec>aac,mp1,mp2,mp3,amr_nb,amr_wb</AudioCodec>
	</Video>
	<Video>
		<Ext>avi</Ext>
		<Demuxer>avi</Demuxer>
		<VideoCodec>hevc,h264,h263,mpeg4,mjpeg,wmv3,xvid</VideoCodec>
		<AudioCodec>aac,mp1,mp2,mp3,ac3,wmav1</AudioCodec>
	</Video>
	<Video>
		<Ext>wmv,asf,wma</Ext>
		<Demuxer>wmv,asf</Demuxer>
		<VideoCodec>h264,mpeg4,xvid,mjpeg,wmv3</VideoCodec>
		<AudioCodec>wmapro,wmav1,wmav2</AudioCodec>
	</Video>
	<Video>
		<Ext>mkv,webm</Ext>
		<Demuxer>matroska</Demuxer>
		<VideoCodec>h264,hevc,mpeg4,mpeg4,wmv3,vc1,vp8</VideoCodec>
		<AudioCodec>aac,mp1,mp2,mp3,ac3,pcm,vorbis</AudioCodec>
	</Video>
	<Video>
		<Ext>mpg,mpeg,vro,vob</Ext>
		<Demuxer>mpegps</Demuxer>
		<VideoCodec>mpeg1video,mpeg2video</VideoCodec>
		<AudioCodec>mp1,mp2,mp3,ac3</AudioCodec>
	</Video>
	<Video>
		<Ext>ts,m2ts</Ext>
		<Demuxer>mpegts</Demuxer>
		<VideoCodec>h264,vc1,mpeg2video,hevc,cavs</VideoCodec>
		<AudioCodec>mp1,mp2,mp3,aac,ac3</AudioCodec>
	</Video>
	<Video>
		<Ext>rm,rmvb</Ext>
		<Demuxer>rm</Demuxer>
		<VideoCodec>rv10,rv20,rv30,rv40</VideoCodec>
		<AudioCodec>aac,cook,ac3</AudioCodec>
	</Video>
	<Video>
		<Ext>ogm,ogg</Ext>
		<Demuxer></Demuxer>
		<VideoCodec>mpeg2video,mp4,h264,mjpeg</VideoCodec>
		<AudioCodec>aac,cook,ac3</AudioCodec>
	</Video>
	<Video>
		<Ext>flv</Ext>
		<Demuxer>flv</Demuxer>
		<VideoCodec>vp6,h264,mpeg4,mjpeg,flv1,svq1,svq3</VideoCodec>
		<AudioCodec>mp1,mp2,mp3,aac</AudioCodec>
	</Video>
</Policy>